    "a (simshadows snippets)": {
        "prefix": ["a"],
        "body": [
            "<a href=\"${2:url}\" target=\"_blank\">$1</a>$0"
        ]
    },
    "blockquote (simshadows snippets)": {
        "prefix": ["blockquote"],
        "body": [
            "<blockquote cite=\"${2:url}\">",
            "\t$1",
            "</blockquote>$0"
        ]
    },
    "button (simshadows snippets)": {
        "prefix": ["button"],
        "body": [
            "<button type=\"button\">${1:text}</button>$0"
        ]
    },
    "canvas (simshadows snippets)": {
        "prefix": ["canvas"],
        "body": [
            "<canvas id=\"${1:id}\">",
            "\tYour browser does not support the canvas tag.",
            "</canvas>$0"
        ]
    },
    "table (simshadows snippets)": {
        "prefix": ["table", "tab"],
        "body": [
            "<table>$1",
            "\t<thead>",
            "\t\t<tr>",
            "\t\t\t<th>head1</th>",
            "\t\t\t<th>head2</th>",
            "\t\t</tr>",
            "\t</thead>",
            "\t<tbody>",
            "\t\t<tr>",
            "\t\t\t<td rowspan=\"2\">cell1</td>",
            "\t\t\t<td>cell2</td>",
            "\t\t</tr>",
            "\t\t<tr>",
            "\t\t\t<td>cell3</td>",
            "\t\t</tr>",
            "\t</tbody>",
            "\t<tfoot>",
            "\t\t<tr>",
            "\t\t\t<td colspan=\"2\">foot</td>",
            "\t\t</tr>",
            "\t</tfoot>",
            "</table>$0"
        ]
    },
    "textarea (simshadows snippets)": {
        "prefix": ["textarea"],
        "body": [
            "<textarea id=\"${1:todo}\" name=\"${2:todo}\">$3</textarea>$0"
        ]
    }
